
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#242424",
          "foreground": "#3A86FF",
          "leading_diamond": " ",
          "properties": {
            "template": "\u2800{{ if .WSL }}WSL at {{ end }}{{.Icon}}\u2800"
          },
          "style": "diamond",
          "type": "os"
        },
        {
          "background": "#242424",
          "foreground": "#33DD2D",
          "properties": {
            "style": "agnoster_full",
            "template": "\u2800\ue5ff {{ .Path }}\u2800"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "background": "#242424",
          "foreground": "#3A86FF",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "template": "{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }}\u2800"
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#512BD4",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": "\u2800\ue77f\u2800{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}\u2800"
          },
          "style": "powerline",
          "type": "dotnet"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": "\u2800\ue718\u2800{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}\u2800"
          },
          "style": "powerline",
          "type": "node"
        },
        {
          "background": "#4063D8",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": "\u2800\ue738\u2800{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}\u2800"
          },
          "style": "powerline",
          "type": "java"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#2e9599",
          "foreground": "#111111",
          "leading_diamond": "\ue0b6",
          "properties": {
            "template": "{{ .CurrentDate | date .Format }}\u2800",
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "newline"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffff66",
          "properties": {
            "root_icon": "\uf0e7",
            "template": " \uf0e7 "
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#00897b",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": " \u276f "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}(Admin) {{end}}{{.Folder}}",
  "final_space": true,
  "version": 1
}
